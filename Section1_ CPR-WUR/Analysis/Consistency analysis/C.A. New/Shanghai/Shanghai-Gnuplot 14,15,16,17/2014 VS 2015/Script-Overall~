set xlabel "Overall score-15" font "Verdana,18"
set ylabel "Overall score-14" font "Verdana,18"
set ylabel offset 1,0
set title "Shanghai 2014-2015" font "Verdana,18" 
set border lw 3
set lmargin at screen 0.15
set xtics font ",14"
set ytics font ",14"
set xtics nomirror
set ytics nomirror
set xrange[0:100]
set yrange[0:100]
set key left top box 1
set datafile separator ","
set term png
set output "Overall.png"
set label "σ1=0.01697" at 80,25 right
set label "σ2=0.02065" at 80,21 right 
plot "Overall.csv" using 3:2 pt 20 ps 1 title "Top100-Non Indian", "line.csv" using 1:2 with line lw 1.85 lc rgb "black" notitle,"Overall.csv" using 5:4 pt 9 title "Top100-Indian"

